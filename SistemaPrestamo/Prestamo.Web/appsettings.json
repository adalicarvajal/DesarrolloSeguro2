{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CadenaSQL": "${ConnectionStrings__CadenaSQL}"
  },
  "Smtp": {
    "Host": "${Smtp__Host}",
    "Port": "${Smtp__Port}",
    "EnableSsl": "${Smtp__EnableSsl}",
    "UserName": "${Smtp__UserName}",
    "Password": "${Smtp__Password}"
  },
  "Jwt": {
    "Key": "${Jwt__Key}",
    "Issuer": "${Jwt__Issuer}",
    "Audience": "${Jwt__Audience}",
    "Expires": "${Jwt__Expires}"
  }
}